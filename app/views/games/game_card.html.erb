<div class="card">
    <div class="card-image">
        <img class="materialboxed" data-caption="<%= @game[:name] %>" src="<%= @game[:image] %>">
    </div>
    <div class="card-content">
        <span class="card-title"><%= @game[:name] %></span>
        <p>Owned by: <%= @game[:owners] %></p>
    </div>
</div>